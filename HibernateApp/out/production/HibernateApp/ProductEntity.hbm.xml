<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ProductEntity" table="product" schema="wish">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="image">
            <column name="image" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="title">
            <column name="title" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="sellId">
            <column name="sell_id" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="cid">
            <column name="cid" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>